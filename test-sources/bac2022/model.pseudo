citeste x,y # (numere naturale, x<y)
u<-0
nr<-0
pentru k<-x,y executa
    s<-0
    nr<-nr+1

    rad <- 1
    cat timp (rad+1)*(rad+1) <= k executa
        rad <- rad + 1
    sf

    pentru t<-1,rad executa
       s<-s+t*t
    sf
    daca s!=u atunci
       scrie s,'*',nr,' '
       u<-s
       nr<-0
    sf
sf